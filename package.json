{
  "name": "vite-build-issue",
  "license": "MIT",
  "version": "0.0.1",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "./dist/vite-build-issue.umd.js",
  "module": "./dist/vite-build-issue.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/vite-build-issue.es.js",
      "require": "./dist/vite-build-issue.umd.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./src/utils": "./src/utils",
    "./dist/theme": "./dist/theme/theme.cjs"
  },
  "scripts": {
    "dev": "vite",
    "build:lib": "LIB_NAME=lib vite build",
    "build:theme": "LIB_NAME=theme vite build",
    "build:all": "npm run build:theme && npm run build:lib && npm run build:theme && node move-files.js",
    "build": "tsc && npm run build:all",
    "types:check": "tsc --skipLibCheck --noEmit",
    "watch": "LIB_NAME=theme vite build --watch --mode dev",
    "preview": "vite preview",
    "format": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx}\""
  },
  "resolutions": {
    "jest-mock": "^28.1.0"
  },
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.3",
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0",
    "tailwind-variants": "^0.2.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@vitejs/plugin-react": "^4.2.1",
    "fs-extra": "^11.2.0",
    "prettier": "^2.8.1",
    "typescript": "^4.6.4",
    "vite": "^5.2.7",
    "vite-plugin-dts": "^1.6.4",
    "vite-tsconfig-paths": "^3.5.1"
  }
}
